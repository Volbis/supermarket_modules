{
  "name": "supermark",
  "version": "0.1.0",
  "private": true,
  "main": "../electron/main.js",
  
  "scripts": {
    "serve": "vue-cli-service serve --port 8081 --host localhost --no-open",
    "build": "vue-cli-service build",
    "electron": "wait-on http-get://localhost:8081 && cross-env ELECTRON_START_URL=http://localhost:8081 npx electron ./electron/main.js",
    "electron:dev": "concurrently -k \"npm:serve\" \"npm:electron\""
  },

  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^3.2.13",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "electron": "^31.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "wait-on": "^7.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}